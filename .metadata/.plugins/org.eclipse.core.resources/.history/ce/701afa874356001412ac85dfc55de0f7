/**
 * @author Josh
 *
 */
public class PokemonQuiz {
	
	/**
	 * @param args
	 * @param keyboard 
	 */
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		double pkmnmaster = 0;
		double loser = 0;
		String ans = null;

		System.out.println("Welcome to the pokemon quiz!");
		Scanner keyboard = new Scanner(System.in);

		System.out.println("Do you like pokemon?! Yes or No");
		ans = keyboard.nextLine();
		
		while (!keyboard.hasNextLine())
		{
			if(!ans.equalsIgnoreCase("Yes") && !ans.equalsIgnoreCase("No")) {
				System.out.println("Answer the question fool!");
			}
			
			
		}
		System.out.println("Good! Lets get started");  
	
		
	}
}
